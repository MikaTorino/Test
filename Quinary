public class Quinary {
	ArrayList<Integer> quinaryArray = new ArrayList<>();
	
	//ジャバーランド語を5進数に変換する
	public ArrayList convertQuinary(ArrayList calcArray) {
		for (StringBuilder s : calcArray) {
			StringBuilder q = null;
			int numOfChars = s.length();
			for (int i = 0; i < numOfChars; i++) {
				//文字列をEnumに変換し5進数を読み込む
				JavalandNum enumNum = JavalandNum.valueOf(s.charAt(i));
				quinaryArray.add(enumNum.getJavalandNum()));
			}
		}
		return quinaryArray;
	}
	
	//変換した5進数をStringBuilderに入れる
